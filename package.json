{
  "name": "hanako-wp",
  "version": "1.6.7",
  "description": "A basic wordpress template based on Timber/Twig and Hanako ts framework.",
  "homepage": "https://github.com/harkle/hanako-wp",
  "bugs": {
    "url": "https://github.com/harkle/hanako-wp/issues"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production --watch",
    "format": "prettier --write views/ts/**/*.ts views/scss/**/*.scss",
    "clean": "rimraf dist",
    "lint:ts": "eslint views/ts/**/*.ts",
    "lint:scss": "stylelint views/scss/**/*.scss"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.15",
    "copy-webpack-plugin": "^12.0.2",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "cssnano": "^6.0.1",
    "eslint": "^8.45.0",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss": "^8.4.27",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "sass": "^1.64.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "stylelint": "^16.13.2",
    "stylelint-config-property-sort-order-smacss": "^10.0.0",
    "stylelint-order": "^6.0.4",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-manifest-plugin": "^5.0.0",
    "webpackbar": "^5.0.2"
  },
  "dependencies": {
    "@types/bootstrap": "^5.2.10",
    "bootstrap": "^5.3.3",
    "hanako-ts": "^1.2.7"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
